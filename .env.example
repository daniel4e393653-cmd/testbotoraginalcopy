### RPC Configuration (REQUIRED)
JSON_RPC_ENDPOINT=https://fullnode.mainnet.sui.io:443

LOG_LEVEL=info

### Worker options
# Protocol options: FLOWX_V3, CETUS, TURBOS_FIANCE, BLUEFIN, MAGMA_FINANCE
PROTOCOL=FLOWX_CLMM
# For CETUS protocol, use: PROTOCOL=CETUS
# Example CETUS pool: 0xb8d7d9e66a60c239e7a60110efcf8de6c705580ed924d0dde141f4a0e2c90105
TARGET_POOL=0x88cec280ed5406af7951ef768b305de5323b843cc127bcab988d08770d00a5f7
BPRICE_PERCENT=50                         # Bottom price percent (basis points). For Cetus, use 500 (0.5%) or 5000 (5%)
TPRICE_PERCENT=10                         # Top price percent (basis points). For Cetus, use 500 (0.5%) or 5000 (5%)
SLIPPAGE_TOLERANCE=50000                  # 5% - Slippage tolerance in basis points
PRICE_IMPACT_PERCENT_THRESHOLD=-5000      # -0.5% - Price impact threshold in basis points
MIN_ZAP_AMOUNT_X=10000                    # Minimum amount for token X (adjust based on token decimals)
MIN_ZAP_AMOUNT_Y=10000                    # Minimum amount for token Y (adjust based on token decimals)
REBALANCE_RETRIES=3
MULTIPLIER=1
REWARD_THRESHOLD_USD=1                    # Minimum USD value of rewards to collect
COMPOUND_REWARDS_SCHEDULE_MS=3600000      # Compound rewards every hour (3600000 ms)
PRIVATE_KEY=                              # Your wallet private key (REQUIRED - keep this secret!)
TRACKING_VOLUME_ADDRESS=
